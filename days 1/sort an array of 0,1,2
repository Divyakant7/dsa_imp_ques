void sortColors(vector<int>& nums) {
        int x=0,y=0,z=0;
    for(int i=0;i<nums.size();i++){
        if(nums[i]==0){
            x++;
        }
        else if(nums[i]==1){
            y++;
        }
        else{
            z++;
        }
        
    }
    int i=0;
        while(x--){
            nums[i] = 0;
            i++;
        }
        while(y--){
            nums[i] = 1;
            i++;
        }
        while(z--){
            nums[i] = 2;
            i++;
        } 
    }
